{
  "version": 2,
  "projects": {
    "class-api": {
      "root": "apps/class-api",
      "projectType": "application",
      "sourceRoot": "apps/class-api",
      "targets": {
        "build": {
          "executor": "@nx-dotnet/core:build",
          "options": { "output": "dist/ClassAPI", "configuration": "Debug" },
          "configurations": { "production": { "configuration": "Release" } }
        },
        "test": { "executor": "@nx-dotnet/core:test" }
      }
    },
    "class-dto": {
      "root": "libs/class-dto",
      "projectType": "library",
      "sourceRoot": "libs/class-dto",
      "targets": {
        "build": {
          "executor": "@nx-dotnet/core:build",
          "options": { "output": "dist/class-dto", "configuration": "Debug" },
          "configurations": { "production": { "configuration": "Release" } }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/workspace" }
}
